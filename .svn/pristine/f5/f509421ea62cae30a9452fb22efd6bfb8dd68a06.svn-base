/*基础样式*/
.header {
  overflow: hidden;
}
.header .logo {
  position: relative;
  width: 300px;
  margin: 10px 0;
  height: 60px;
  float: left;
}
.header .logo a {
  display: block;
  width: 170px;
  height: 60px;
  background-color: papayawhip;
  text-align: center;
  line-height: 60px;
}
.header .logo b {
  display: block;
  width: 110px;
  height: 40px;
  position: absolute;
  right: 0;
  bottom: 0;
}
.header a {
  font-size: 18px;
}
.header .q-link {
  position: relative;
  color: #999;
  float: right;
  top: 52px;
}
.content .login-warp {
  position: relative;
  height: 475px;
  margin: 10px 0 20px;
  z-index: 5;
}
.content .login-warp .login-form {
  float: right;
  top: 40px;
  position: relative;
  z-index: 4;
  background: #fff;
  overflow: visible;
  width: 346px;
  right: 50%;
  margin-right: -173px;
}
.content .login-warp .login-form .login-tab {
  height: 54px;
  font-size: 18px;
  text-align: center;
  border-bottom: 1px solid #f4f4f4;
  position: absolute;
  background: #fff;
  display: block;
  width: 173px;
}
.content .login-warp .login-form .login-tab a {
  width: 99%;
  height: 22px;
  position: absolute;
  left: 0;
  top: 18px;
  border-right: 1px solid #f4f4f4;
  text-decoration: none;
}
.content .login-warp .login-form .login-tab-r {
  right: 0;
}
.content .login-warp .login-form .login-box {
  width: 306px;
  padding: 20px;
  margin-top: 55px;
}
.content .login-warp .login-form .form .item {
  width: 300px;
  height: 44px;
  line-height: 44px;
  position: relative;
  font-weight: normal;
  margin: 30px auto;
}
.content .login-warp .login-form .form .item input {
  width: 240px;
  height: 30px;
  line-height: 30px;
  font-size: 16px;
  outline: none;
  padding-left: 8px;
  box-sizing: border-box;
}
.content .login-warp .login-form .form .item:last-child .btn {
  float: left;
  margin-left: 6.666%;
  width: 40%;
  height: 100%;
  background-color: #f10215;
  line-height: 44px;
  text-align: center;
  font-size: 16px;
  color: #fff;
  cursor: pointer;
  top: 0;
}
.content .login-warp .login-form .form .item:last-child .btn:hover {
  background-color: #eee;
  color: #f10215 ;
}
.content .login-warp .login-form .login-bt {
  padding-left: 20px;
  padding-right: 20px;
  line-height: 50px;
  border-top: 1px solid #f4f4f4;
  height: auto;
  background-color: #fcfcfc;
}
.content .login-warp .login-form .login-bt li {
  float: left;
}
.content .login-warp .login-form .login-bt li a {
  display: inline-block;
  position: relative;
  padding-left: 24px;
  font-size: 14px;
}
.content .login-warp .login-form .login-bt li a:hover {
  color: #666;
}
.content .login-warp .login-form li:last-child {
  float: right;
}
.content .login-warp .login-form li:last-child a {
  color: #f10215;
}
